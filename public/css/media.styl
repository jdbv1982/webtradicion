@media screen and (min-width:768px)
	nav 
		background none
		text-align center
		border-bottom 1px solid letras
		position relative
		width 60%
		left 20%
		
		.decoration-left
			border-left 2px solid letras			
			

	.div-mensaje
		top: 50px;

	.web-div-header
